{
    "name": "muratbsts/laravel-reactable",
    "description": "Use reactions for your Laravel model like Facebook, without pain",
    "keywords": [
        "laravel",
        "reactions",
        "reactable",
        "facebook-reactions"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Murat Bastas",
            "email": "muratbsts@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Muratbsts\\Reactable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Muratbsts\\Reactable\\Providers\\ReactableServiceProvider"
            ]
        }
    },
    "require": {
        "illuminate/database": "^5.2.0",
        "illuminate/contracts": "^5.2.0",
        "illuminate/support": "^5.2.0"
    }
}
